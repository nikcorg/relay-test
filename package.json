    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.8.0",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "express-graphql": "^0.5.1",
    "graphql": "^0.5.0",
    "graphql-relay": "^0.4.1",
    "history": "^2.1.0",
    "ramda": "^0.21.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-relay": "^0.8.0",
