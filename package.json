    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "envify": "^3.4.0",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^5.0.0",
    "parallelshell": "^2.0.0",
    "watchify": "^3.7.0"
